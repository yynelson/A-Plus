{
  "name": "A Plus",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Get pages source from a popup",
  "browser_action": {
    "default_icon": "aplus_resized.png",
    "default_popup": "popup.html"
  },
  "permissions": ["tabs", "<all_urls>","storage"],
  "content_scripts": 
    [
        {
            "js":["jquery.js"],
            "matches": [ "http://*/*"]
        }
    ]

}